import java.io.*;
import java.util.*;






public class Solution {
    public static void main(String[] args) throws IOException {
       Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
       int ar[]=new int[n];
       long sum=0;
       for(int i=0;i<n;i++)
       {
           ar[i]=sc.nextInt();
       }
       for(int i=0;i<n;i++)
       {
         sum=sum+ar[i];  
       }
       System.out.println(sum);
    }
}
